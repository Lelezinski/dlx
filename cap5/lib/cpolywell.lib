.param capaw     = {coxsti*wolapw*(l-dl)}
.param capab     = {coxsti*wolapb*(l-dl)}
*-------------------------------------------------------------------------------
* Netlist
*-------------------------------------------------------------------------------
Erat  rat  0 	  value={(sub*v(PLUS,MINUS)+vfbt)/vsm}
Eexp  ex   0 	  value={0.5*((-sub*v(PLUS,MINUS)-vfbt)*sl2+sqrt((sl2^2)*(-sub*v(PLUS,MINUS)-vfbt)^2 + eps^2))}
EVacc acc  0 	  value={cmax/(1.0+(exp(-(-sub*v(PLUS,MINUS)-vfbt)/(sl1+v(ex))))*(cmax-cfb)/cfb)}
EVdep dep  0 	  value={cfb/(1.0+(valif(v(rat)>20,v(rat),log(1.0+exp(v(rat)))))*vsm/vnorm)^mexp}

Cc    PLUS MINUS  value={(v(dep)/(1.0+exp(-v(rat)))+v(acc)/(1.0+exp(v(rat))))*number}
Cpw   PLUS MINUS  {(cperi+capaw)*number}

Cpb   PLUS B      {capab*number}
Dnw   B    MINUS  DNWPS area={areanw*number} peri={perinw*number}

C1    rat  0      1e-15
C2    ex   0      1e-15
C3    acc  0      1e-15
C4    dep  0      1e-15

.ends CDONWPO3V3M1


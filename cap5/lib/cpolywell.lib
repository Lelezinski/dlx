.param wolapb    = {max(cep+wcon+dcoa-dl/2.0-(danw-dwnw/2.0),0)}
.param lnw       = {l+2.0*(dcop+wcon+cea+danw)}
.param wnw       = {w+2.0*danw}
.param areanw    = {(wnw-dwnw)*(lnw-dwnw)}
.param perinw    = {2.0*(wnw-dwnw+lnw-dwnw)}

.param area      = {(w-dw)*(l-dl)}
.param peri      = {2.0*(w-dw)}
*
*-------------------------------------------------------------------------------
* Computing intrinsic capa between Poly & Well
*-------------------------------------------------------------------------------
* Intermediate computations using BSIM3 equations for eg, ni, phi
.param tempk     = {temper+273.15}
.param trefk     = {tref+273.15}
.param eg        = {1.16-7.02e-4*(tempk^2)/(tempk+1108)} ! (eV)
.param egr       = {1.16-7.02e-4*(trefk^2)/(trefk+1108)} ! (eV)
.param vtemp     = {(kb*tempk)/q}
.param vtempr    = {(kb*trefk)/q}
.param ni        = {1.45e10*((tempk/300.15)^1.5)*exp(21.556981-eg/(2*vtemp))}
.param nir       = {1.45e10*((trefk/300.15)^1.5)*exp(21.556981-egr/(2*vtempr))}
.param phi       = {vtemp*log(nsub/ni)}
.param phir      = {vtempr*log(nsub/nir)}
.param vfbt      = {vfb+(phi-phir)}
.param cox       = {epsi_ox/tox}
.param coxsti    = {epsi_ox/tox_sti}
.param cmax      = {cox*area}
.param ld        = {sqrt(epsi_si*vtemp/(q*nsub*1e6))}
.param cfbs      = {epsi_si*area/ld}
.param cfb       = {cmax*cfbs/(cmax+cfbs)}
.param vnorm     = {q*nsub*1e6*epsi_si/(2.0*cox^2)}
